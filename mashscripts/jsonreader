echo "Enter the file path: "
read filepath

keys=$(jq -r 'keys_unsorted | .[]' "filepath")

echo "Keys found: $keys"

echo "Enter the key you wanna see:"
read userkey

if [["$keys"=~"$userkey"]]; then
value = $(jq -r ".$userkey" "$filepath")
echo "Values for '$userkey':$value"
else
echo "No such key!"
exit 1
fi
